<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="template_header_style_4" name="Header Style Conedera" inherit_id="theme_prime.template_header_style_4">
        <xpath expr="//div[text()='My Cart']" position="replace">
            <div class="mb-1">Mi Carrito</div>
        </xpath>
        
        <xpath expr="//div[text()='My Wishlist']" position="replace">
            <div class="mb-1">Mi Lista de Deseos</div>
        </xpath>
        
        <xpath expr="//p[text()='View Wishlist']" position="replace">
            <p class="h6 mb-0">Ver Lista de Deseos</p>
        </xpath>
        
        <xpath expr="//p[text()='My Account']" position="replace">
            <p class="h6 mb-0">Mi Cuenta</p>
        </xpath>
    </template>

</odoo>
